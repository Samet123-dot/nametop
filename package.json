{
  "name": "youtube-music-downloader",
  "version": "1.0.0",
  "description": "Modern YouTube Music Downloader with Dark Mode",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ytdl-core": "^4.11.5",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["youtube", "music", "downloader", "dark-mode", "react", "nodejs", "express"],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youtube-music-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/youtube-music-downloader/issues"
  },
  "homepage": "https://github.com/yourusername/youtube-music-downloader#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
} 